<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hamsemare.appkiller.GameActivity">

    <EditText
        android:id="@+id/guess1"
        android:layout_width="254dp"
        android:layout_height="73dp"
        android:ems="10"
        android:inputType="text|textPersonName|number"
        android:text="Guess:"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="609dp" />

    <ImageView
        android:id="@+id/img1"
        android:layout_width="497dp"
        android:layout_height="497dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toTopOf="@+id/editText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img1level1" />

</android.support.constraint.ConstraintLayout>
